/* pages/camera/camera.wxss */
.container{
  padding: 0;
}

.mainButton button {
  width: 70px !important;
  height: 70px;
  border-radius: 50%;
  padding: 5px;
  border: 1px solid #a296e5;
  background-color: currentColor;
  background-clip:content-box;
}

.button1-hover{
  position: relative;
  top: 3rpx;
  box-shadow:0px 0px 4px #999 inset;
}

.mainButton progress {
  margin-top: 1vh;
  width: 70vw;
}

.button1 {
  background: linear-gradient(to right, #a296e5, #95edf8);
  color: #fff;
}

.button1::after {
  border: none;
}

